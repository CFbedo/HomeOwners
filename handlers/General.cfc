<cfcomponent output="false">	<!--- Default Action --->	<cffunction name="index" returntype="void" output="false" hint="My main event">		<cfargument name="event" required="true">		<cfset var rc = event.getCollection()>				<cfset rc.welcomeMessage = "Welcome to ColdBox!">					<cfset event.setView("General/index")>	</cffunction>	<cffunction name="secondPage" returntype="void" output="false" hint="My main event">		<cfargument name="event" required="true">		<cfset var rc = event.getCollection()>				<cfset rc.bedo = "bedo ">					<cfset event.setView("General/secondPage")>	</cffunction><!------------------------------------------- PRIVATE EVENTS ------------------------------------------>
	</cfcomponent>